{
    "compilerOptions": {
      "target": "ES6",
      "outDir": "./build/",
      "module": "Commonjs",
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "esModuleInterop": true,
      "resolveJsonModule": true
    }
}